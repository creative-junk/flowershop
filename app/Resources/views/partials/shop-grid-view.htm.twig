<div class="row">
    {% for product in products %}
        <div class="col-lg-4 col-xs-12 col-sm-6 product-holder">
            {{ include(':partials:shop-grid-item.html.twig',{'product':product} ) }}
        </div>
    {% else %}
        <div class="col-xs-12">
            <h2 class="empty">There are no products in this category.</h2>
        </div>
    {% endfor %}
</div>