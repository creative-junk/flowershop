        <script type="text/javascript">

            $(window).scroll(function () {
                if ($(this).scrollTop() > 300) {
                    $('#back_top').fadeIn();
                } else {
                    $('#back_top').fadeOut();
                }
            });
            $('#back_top').click(function () {
                $('body,html').animate({scrollTop: 0}, 500);
            });


